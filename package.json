    "@types/lodash.merge": "4.6.6",
    "@types/node": "16.10.2",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "chromatic": "5.10.2",
    "commitlint": "13.2.1",
    "copyfiles": "2.4.1",
