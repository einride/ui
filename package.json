{
  "name": "@einride/ui",
  "version": "0.0.0-development",
  "description": "Einride's UI framework with reusable components",
  "scripts": {
    "build": "tsc -p tsconfig.json && yarn copy-files",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token f99937c406c4",
    "commitlint": "commitlint --from origin/main --to HEAD",
    "copy-files": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.svg\" \"src/**/*.png\" \"src/assets/fonts/EinrideIcons-Book.woff\"  lib/",
    "git-verify-nodiff": "./scripts/git-verify-nodiff.bash",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "prettier": "prettier --write .",
    "review": "yarn install --frozen-lockfile && yarn commitlint && yarn prettier && yarn eslint && yarn git-verify-nodiff && yarn build",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "react-scripts test"
  },
  "dependencies": {
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
