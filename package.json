{
  "name": "@einride/ui",
  "version": "0.0.0-development",
  "description": "Einride's UI framework with reusable components",
  "scripts": {
    "build": "rollup --config",
    "build-storybook": "build-storybook",
    "commitlint": "commitlint --from origin/main --to HEAD",
    "develop": "yarn storybook",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ext ts,tsx src/",
    "prettier": "prettier --write .",
    "review": "yarn install --frozen-lockfile && concurrently --kill-others-on-fail 'yarn commitlint' 'yarn format-check' 'yarn lint' 'yarn test' 'yarn typecheck' 'yarn build'",
    "storybook": "start-storybook -p 6006",
    "test": "vitest run",
    "test-watch": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@einride/hooks": "1.7.0",
    "@emotion/is-prop-valid": "1.2.0",
    "@mantine/hooks": "5.5.4",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@einride/eslint-plugin": "6.10.1",
    "@einride/prettier-config": "2.0.0",
    "@einride/tsconfig": "1.0.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@rollup/plugin-image": "2.1.1",
    "@rollup/plugin-typescript": "8.5.0",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addons": "6.5.12",
    "@storybook/builder-vite": "0.2.3",
    "@storybook/node-logger": "6.5.12",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@storybook/theming": "6.5.12",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash.merge": "4.6.7",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "chromatic": "6.10.1",
    "concurrently": "7.4.0",
    "eslint": "8.24.0",
    "eslint-plugin-storybook": "0.6.4",
    "framer-motion": "7.5.2",
    "jsdom": "20.0.1",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "2.79.1",
    "semantic-release": "19.0.5",
    "storybook-dark-mode": "1.1.2",
    "typescript": "4.8.4",
    "vite": "3.1.6",
    "vitest": "0.24.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^6.2.8 || ^7.0.0",
    "react": "^18.0.0"
  },
  "main": "./dist/cjs/main.js",
  "module": "./dist/esm/main.js",
  "exports": {
    ".": {
      "import": "./dist/esm/main.js",
      "require": "./dist/cjs/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "16 || 18"
  },
  "author": "Filip Tammerg√•rd",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "component-library",
    "ui",
    "react",
    "typescript",
    "einride",
    "styleguide"
  ],
  "license": "MIT",
  "homepage": "https://github.com/einride/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/ui.git"
  },
  "bugs": {
    "email": "open-source@einride.tech",
    "url": "https://github.com/einride/ui/issues"
  }
}
