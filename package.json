{
  "name": "@einride/ui",
  "version": "0.0.0-development",
  "description": "Einride's UI framework with reusable components",
  "scripts": {
    "build": "rollup --config",
    "build-storybook": "build-storybook",
    "commitlint": "commitlint --from origin/main --to HEAD",
    "develop": "yarn storybook",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ext ts,tsx src/",
    "review": "yarn install --frozen-lockfile && concurrently --kill-others-on-fail 'yarn commitlint' 'yarn format-check' 'yarn lint' 'yarn test' 'yarn typecheck' 'yarn build'",
    "storybook": "start-storybook -p 6006",
    "test": "vitest run",
    "test-storybook": "test-storybook",
    "test-storybook-coverage": "test-storybook --coverage",
    "test-watch": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@einride/hooks": "1.7.5",
    "@emotion/is-prop-valid": "1.2.0",
    "@mantine/core": "5.10.4",
    "@mantine/dates": "5.10.4",
    "@mantine/hooks": "5.10.4",
    "@radix-ui/react-alert-dialog": "1.0.2",
    "@radix-ui/react-dialog": "1.0.2",
    "@radix-ui/react-dropdown-menu": "2.0.2",
    "@radix-ui/react-slider": "1.1.0",
    "@radix-ui/react-switch": "1.0.1",
    "@radix-ui/react-tabs": "1.0.2",
    "@radix-ui/react-tooltip": "1.0.3",
    "dayjs": "1.11.7",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.3",
    "@einride/eslint-plugin": "6.21.0",
    "@einride/prettier-config": "2.0.0",
    "@einride/tsconfig": "1.0.1",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@faker-js/faker": "7.6.0",
    "@rollup/plugin-image": "3.0.2",
    "@rollup/plugin-typescript": "11.0.0",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-coverage": "0.0.8",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/jest": "0.0.10",
    "@storybook/node-logger": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/test-runner": "0.10.0-next.8",
    "@storybook/testing-library": "0.0.13",
    "@storybook/theming": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash.merge": "4.6.7",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "chromatic": "6.17.0",
    "concurrently": "7.6.0",
    "eslint": "8.34.0",
    "eslint-plugin-storybook": "0.6.10",
    "framer-motion": "9.0.4",
    "happy-dom": "8.6.0",
    "luxon": "3.2.1",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook-dark-mode": "2.1.0",
    "typescript": "4.9.5",
    "vite": "4.1.3",
    "vite-plugin-turbosnap": "1.0.1",
    "vitest": "0.28.5"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^6.2.8 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "react": "^18.0.0"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/main.d.ts",
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.esm.js",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.esm.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "engines": {
    "node": "16 || 18"
  },
  "author": "Filip Tammerg√•rd",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "component-library",
    "ui",
    "react",
    "typescript",
    "einride",
    "styleguide"
  ],
  "resolutions": {
    "@testing-library/user-event": "14.4.3"
  },
  "license": "MIT",
  "homepage": "https://github.com/einride/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/ui.git"
  },
  "bugs": {
    "email": "open-source@einride.tech",
    "url": "https://github.com/einride/ui/issues"
  }
}
