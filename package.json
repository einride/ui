{
  "name": "@einride/ui",
  "version": "0.0.0-development",
  "description": "Einride's UI framework with reusable components",
  "main": "./lib/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && yarn copy-files",
    "build-storybook": "build-storybook",
    "commitlint": "commitlint --from origin/main --to HEAD",
    "copy-files": "copyfiles -u 1 \"src/**/*.svg\" lib/",
    "develop": "yarn storybook",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ext ts,tsx src/",
    "prettier": "prettier --write .",
    "review": "yarn install --frozen-lockfile && concurrently --kill-others-on-fail 'yarn commitlint' 'yarn format-check' 'yarn lint' 'yarn build'",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@einride/hooks": "1.4.5",
    "@mantine/hooks": "5.0.2",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@einride/eslint-plugin": "6.5.0",
    "@einride/prettier-config": "2.0.0",
    "@einride/tsconfig": "1.0.0",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.10",
    "@storybook/addons": "6.5.9",
    "@storybook/builder-vite": "0.2.2",
    "@storybook/node-logger": "6.5.10",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@storybook/theming": "6.5.10",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.1",
    "@types/jest": "28.1.6",
    "@types/lodash.merge": "4.6.7",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "chromatic": "6.7.3",
    "concurrently": "7.3.0",
    "copyfiles": "2.4.1",
    "eslint": "8.21.0",
    "eslint-plugin-storybook": "0.6.3",
    "framer-motion": "6.5.1",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "19.0.3",
    "storybook-dark-mode": "1.1.0",
    "typescript": "4.7.4",
    "vite": "3.0.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^6.2.8",
    "react": ">=17.0.0"
  },
  "module": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "16"
  },
  "author": "Filip Tammerg√•rd",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "component-library",
    "ui",
    "react",
    "typescript",
    "einride",
    "styleguide"
  ],
  "license": "MIT",
  "homepage": "https://github.com/einride/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/ui.git"
  },
  "bugs": {
    "email": "open-source@einride.tech",
    "url": "https://github.com/einride/ui/issues"
  }
}
