{
  "name": "@einride/ui",
  "version": "0.0.0-development",
  "description": "Einride's UI framework with reusable components",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && yarn copy-files",
    "build-storybook": "build-storybook",
    "commitlint": "commitlint --from origin/main --to HEAD",
    "copy-files": "copyfiles -u 1 \"src/**/*.svg\" lib/",
    "develop": "yarn storybook",
    "git-verify-nodiff": "./scripts/git-verify-nodiff.bash",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "prettier": "prettier --write .",
    "review": "yarn install --frozen-lockfile && yarn commitlint && yarn prettier && yarn lint && yarn git-verify-nodiff && yarn build",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@mantine/hooks": "4.1.3",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@einride/eslint-plugin": "5.0.0",
    "@einride/prettier-config": "1.2.0",
    "@einride/tsconfig": "1.0.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@storybook/addon-a11y": "6.4.21",
    "@storybook/addon-actions": "6.4.21",
    "@storybook/addon-essentials": "6.4.21",
    "@storybook/addon-links": "6.4.21",
    "@storybook/addons": "6.4.21",
    "@storybook/node-logger": "6.4.21",
    "@storybook/react": "6.4.21",
    "@storybook/testing-library": "0.0.9",
    "@storybook/theming": "6.4.21",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.1",
    "@testing-library/user-event": "14.1.0",
    "@types/jest": "27.4.1",
    "@types/lodash.merge": "4.6.6",
    "@types/react": "18.0.3",
    "@types/react-dom": "18.0.0",
    "chromatic": "6.5.4",
    "copyfiles": "2.4.1",
    "eslint": "8.13.0",
    "eslint-plugin-storybook": "0.5.8",
    "framer-motion": "6.2.10",
    "prettier": "2.6.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "semantic-release": "19.0.2",
    "storybook-addon-designs": "6.2.1",
    "storybook-builder-vite": "0.1.23",
    "storybook-dark-mode": "1.0.9",
    "typescript": "4.6.3",
    "vite": "2.9.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^6.2.8",
    "react": ">=17.0.0"
  },
  "main": "./lib/main.js",
  "module": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "16"
  },
  "author": "Filip Tammerg√•rd",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "component-library",
    "ui",
    "react",
    "typescript",
    "einride",
    "styleguide"
  ],
  "license": "MIT",
  "homepage": "https://github.com/einride/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/ui.git"
  },
  "bugs": {
    "email": "open-source@einride.tech",
