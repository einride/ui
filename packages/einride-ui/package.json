{
  "name": "@einride/ui",
  "version": "0.0.0-development",
  "description": "Einride's UI framework with reusable components",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "rollup --config",
    "build-storybook": "storybook build",
    "commitlint": "commitlint --from origin/main --to HEAD",
    "develop": "yarn storybook",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ext ts,tsx src/",
    "review": "concurrently --kill-others-on-fail 'yarn commitlint' 'yarn format-check' 'yarn lint' 'yarn test' 'yarn typecheck' 'yarn build'",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test-storybook": "test-storybook",
    "test-storybook-coverage": "test-storybook --coverage",
    "test-watch": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@einride/hooks": "1.7.5",
    "@emotion/is-prop-valid": "1.2.0",
    "@mantine/core": "6.0.2",
    "@mantine/dates": "6.0.2",
    "@mantine/hooks": "6.0.2",
    "@radix-ui/react-alert-dialog": "1.0.3",
    "@radix-ui/react-dialog": "1.0.3",
    "@radix-ui/react-dropdown-menu": "2.0.4",
    "@radix-ui/react-slider": "1.1.1",
    "@radix-ui/react-switch": "1.0.2",
    "@radix-ui/react-tabs": "1.0.3",
    "@radix-ui/react-tooltip": "1.0.5",
    "dayjs": "1.11.7",
    "lodash.merge": "4.6.2",
    "mapbox-gl": "2.11.0",
    "react-map-gl": "7.0.19"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@einride/eslint-plugin": "6.23.0",
    "@einride/prettier-config": "2.0.0",
    "@einride/tsconfig": "1.0.1",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@faker-js/faker": "7.6.0",
    "@rollup/plugin-image": "3.0.2",
    "@rollup/plugin-typescript": "11.0.0",
    "@storybook/jest": "0.0.11-next.0",
    "@storybook/react": "7.0.0-rc.4",
    "@storybook/testing-library": "0.0.14-next.1",
    "@types/lodash.merge": "4.6.7",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "chromatic": "6.17.2",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "eslint-plugin-storybook": "0.6.11",
    "framer-motion": "10.6.0",
    "happy-dom": "8.9.0",
    "luxon": "3.3.0",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.19.1",
    "typescript": "5.0.2",
    "vite": "4.1.4",
    "vitest": "0.29.7"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": ">=6.2.8",
    "react": "^18.0.0"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/main.d.ts",
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.esm.js",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.esm.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "engines": {
    "node": "16 || 18"
  },
  "author": "Filip Tammerg√•rd",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "component-library",
    "ui",
    "react",
    "typescript",
    "einride",
    "styleguide"
  ],
  "license": "MIT",
  "homepage": "https://github.com/einride/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/ui.git"
  },
  "bugs": {
    "email": "open-source@einride.tech",
    "url": "https://github.com/einride/ui/issues"
  },
  "packageManager": "yarn@3.4.1"
}
